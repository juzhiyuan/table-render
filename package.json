{
  "name": "table-render",
  "version": "0.5.3",
  "description": "中后台表格解决方案",
  "scripts": {
    "start": "dumi dev",
    "site": "dumi build",
    "build": "father-build",
    "predeploy": "npm run site",
    "deploy": "gh-pages -d docs-dist",
    "prepare": "npm run build",
    "postpublish": "git push --tags",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/src/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-card": "^1.8.3",
    "dayjs": "^1.8.29",
    "form-render": "0.9.5",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.33",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0",
    "antd": "^4.4.1",
    "react": "^16.12.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
